        align-items: center;
        display: flex;
        flex-direction: row;
        padding: 0 6px;
    }

    .match-netural-bg {
        border-radius: 6px;
        margin: 0 12px;
        padding: 4px;
    }

    .match-participants {
        display: flex;
        flex-direction: column;
        padding: 2px 0;
        width: 204px;
        min-width: 204px;
        gap: 4px;
    }

    .participant {
        align-items: center;
        display: flex;
        flex-direction: row;
        height: 28px;
    }

    .participant-image {
        height: 28px;
        padding-inline-end: 8px;
        width: 28px;
    }

    .participant-image img {
        width: 100%;
    }

    .participant-name {
        flex: 1;
        font-size: ${i.Sb};
        line-height: ${i.Tb};
        overflow: hidden;
        padding-inline-end: 8px;
        text-overflow: ellipsis;
        white-space: nowrap;
    }

    .score-container {
        display: flex;
        flex-direction: row;
        align-items: center;
    }

    .participant-score {
        font-size: ${i.Sb};
        line-height: ${i.Tb};
        overflow: hidden;
        padding-inline-end: 4px;
        text-align: right;
        text-overflow: ellipsis;
        white-space: nowrap;
    }

    .participant-name.winner,
    .participant-score.winner,
    .game-time {
        font-weight: bold;
    }

    .long-score .participant-name {
        font-size: ${i.Ub};
        line-height: ${i.Vb};
    }

    .long-score .participant-score {
        font-size: ${i.Ub};
        line-height: ${i.Vb};
    }

    .long-score .participant-name.winner,
    .long-score .participant-score.winner {
        font-weight: 600;
    }

    .participant-winner-tag {
        padding-inline-end: 4px;
        width: 5px;
        height: 24px;
    }

    .participant-winner-tag > svg > path {
        fill: currentColor;
    }

    .participant-name.loser,
    .participant-score.loser {
        color: ${i.ub};
    }

    .participant-name.semibold,
    .participant-score.semibold {
        font-weight: 600;
    }

    .match-details {
        font-size: ${i.Ub};
        line-height: ${i.Vb};
        width: 64px;
        color: ${i.ub};
    }

    .detail-attributes {
        display: flex;
        flex-direction: column;
        flex: 1;
        min-width: 0;
        align-items: center;
    }

    .game-clock,
    .game-state,
    .game-date,
    .game-time,
    .game-tv-channel {
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        text-align: center;
        color: ${i.ub};
    }

    .inprogress-game .game-clock {
        color: #cc0000;
        font-weight: bold;
    }

    .winner-tag {
        padding-inline-end: 4px;
        width: 5px;
        height: 24px;
    }

    .winner-tag > svg > path {
        fill: currentColor;
    }

    .detail-live-animation {
        height: 2px;
        background: #cc0000;
        animation: liveAnimation 1.6s ease-out infinite 1.0s alternate;
    }
    @keyframes liveAnimation {
        from {
            width: 100%;
        }
        to {
            width: 2px;
        }
    }

